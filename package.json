	"scripts": {
		"build:push": "chmod +x ./build.sh && ./build.sh",
		"start:docker": "chmod +x ./start.sh && ./start.sh",
		"start:docker:watch": "chmod +x ./start.sh && ./start.sh --watch",
		"dc:up": "docker compose up -d",
		"dc:build": "docker compose up -d --build",
		"dc:down": "docker compose down",
		"dc:destroy": "docker compose down --volumes --remove-orphans --timeout 15",
		"dc:watch": "docker compose build && docker compose up --watch"
